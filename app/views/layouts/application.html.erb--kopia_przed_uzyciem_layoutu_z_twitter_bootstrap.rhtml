<!-- przed wykonaniem '$ rails g bootstrap:layout application fluid', plik application.html.erb wyglądał właśnie tak: -->
<!DOCTYPE html>
<html>
<head>
  <title>TmpDeviseAndNestedModelForms</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<p id="notice"><%= notice %></p>
	<p id="alert"> <%= alert  %></p>

	<%= render 'layouts/nav' %>
	<%= yield %>

</body>
</html>
